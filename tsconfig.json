{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es6",
        "module": "commonjs",
        "outDir": "lib",
        "lib": [
          "es6",
          "dom"
        ]
    },
    "include": [
      "./src/**/*"
    ],
    "exclude": [
      "./packages",
      "./lib",
      "./obj",
      "./bin",
      "./Scripts"
    ]
}